{
  "workbench.iconTheme": "material-icon-theme",
  "explorer.sortOrderLexicographicOptions": "lower", // Case sensitive file ordering in tree
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    140
  ],
  "C_Cpp.clang_format_fallbackStyle": "Visual Studio",
  "cSpell.words": [
    "DEADBAND",
    "Zholob"
  ],
  "files.exclude": {
    "**/.clang-format": true,
    "**/.editorconfig": true,
    "**/.gitignore": true,
    "**/LICENSE": true
  }
}
